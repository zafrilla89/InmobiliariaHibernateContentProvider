<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.izv.android.inmobiliaria" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <provider
            android:name="com.izv.android.inmobiliaria.ProveedorInmueble"
            android:authorities="com.izv.android.inmobiliaria.proveedorinmueble"
            android:enabled="true"
            android:exported="true" >
        </provider>
        <activity
            android:name="com.izv.android.inmobiliaria.Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.izv.android.inmobiliaria.Alta"
            android:label="@string/title_activity_alta" >
        </activity>
        <activity
            android:name="com.izv.android.inmobiliaria.Secundaria"
            android:label="@string/title_activity_secundaria" >
        </activity>
        <activity
            android:name="com.izv.android.inmobiliaria.Editar"
            android:label="@string/title_activity_editar" >
        </activity>
        <activity
            android:name="com.izv.android.inmobiliaria.Hacerfotos"
            android:label="@string/title_activity_hacerfotos" >
        </activity>
    </application>
</manifest>


